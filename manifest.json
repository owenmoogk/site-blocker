{
	"manifest_version": 3,
	"name": "No More Youtube",
	"version": "1.0.0",
	"description": "Owen is not allowed to watch youtube",

	"action":{
		"default_popup": "popup/popup.html"
	},

	"content_scripts": [
		{
			"matches": [
				"https://*/*",
				"http://*/*"
			],
			"js": [
				"content.js"
			]
		}
	],

	"background":{
		"service_worker": "background.js"
	},

	"permissions": [
		"tabs",
		"storage"
	]
}