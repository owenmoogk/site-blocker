{
	"manifest_version": 2,
	"name": "No More Youtube",
	"version": "1.0.0",
	"description": "Owen is not allowed to watch youtube",

	"content_scripts": [
		{
			"matches": [
				"https://*/*",
				"http://*/*"
			],
			"js": [
				"content.js"
			]
		}
	],

	"background":{
		"scripts": ["background.js"]
	},

	"permissions": [
		"tabs"
	]
}